{
  "name": "react_sovremennik",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://sovremennik.sbor.net/newSovrem",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lite-youtube-embed": "^2.0.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "swiper": "^6.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze_js": "source-map-explorer 'build/static/js/*.js'",
    "analyze_css": "source-map-explorer 'build/static/css/*.css' --no-border-checks",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "scrapper": "node ExternalScripts/Scrapper/scrapper",
    "make_ps": "node ExternalScripts/PhotoShopScedule/makePSscedule",
    "get_scedule": "node ExternalScripts/Excel/insertSceduleFromXLSX",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "cherio": "^1.0.0-rc.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "fs": "^0.0.1-security",
    "gh-pages": "^2.2.0",
    "puppeteer": "^8.0.0",
    "source-map-explorer": "^2.5.2",
    "xlsx": "^0.16.8"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "./config/setupTest.js"
    ],
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
